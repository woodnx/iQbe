{
  "name": "api",
  "version": "0.1.0",
  "description": "",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "require": "./dist/types.js",
      "import": "./dist/types.mjs",
      "types": "./dist/types.d.ts"
    },
    "./predicates": {
      "require": "./dist/predicates.js",
      "import": "./dist/predicates.mjs",
      "types": "./dist/predicates.d.ts"
    },
    "./schema": {
      "require": "./dist/schema.js",
      "import": "./dist/schema.mjs",
      "types": "./dist/schema.d.ts"
    }
  },
  "devDependencies": {
    "openapi-fetch": "^0.13.5",
    "openapi-typescript": "^7.3.0",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.5.4"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run clean && npm run codegen && npm run build:tsup",
    "build:tsup": "tsup-node --config tsup.config.ts",
    "clean": "rm -rf dist && rm -rf ./src/gen",
    "codegen": "npm run codegen:openapi-ts",
    "codegen:openapi-ts": "tsx ./src/transform.ts"
  },
  "author": "",
  "license": "ISC",
  "files": [
    "dist"
  ]
}
