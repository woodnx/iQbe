{
  "name": "iqbe",
  "version": "4.1.5",
  "description": "A powerful tool to enhance your quiz skills more fun and effective. ",
  "workspaces": [
    "packages/frontend",
    "packages/backend",
    "packages/api"
  ],
  "packageManager": "pnpm@10.18.3",
  "private": true,
  "scripts": {
    "test": "pnpm --filter ./packages/backend run test",
    "preinstall": "npx only-allow pnpm",
    "build": "./node_modules/.bin/env-cmd -f .config/.env node ./scripts/build.mjs",
    "start": "./node_modules/.bin/env-cmd -f .config/.env pnpm --filter ./packages/backend run start",
    "clean": "node ./scripts/clean.mjs",
    "dev": "./node_modules/.bin/env-cmd -f .config/.env node ./scripts/dev.mjs",
    "api": "pnpm --filter ./packages/api run build",
    "migrate:create": "./node_modules/.bin/env-cmd -f .config/.env pnpm --filter ./packages/backend run migrate:create",
    "migrate:exe": "./node_modules/.bin/env-cmd -f .config/.env pnpm --filter ./packages/backend run migrate:exe",
    "migrate:dev": "./node_modules/.bin/env-cmd -f .config/.env pnpm --filter ./packages/backend run migrate:exe",
    "migrate:status": "./node_modules/.bin/env-cmd -f .config/.env pnpm --filter ./packages/backend run migrate:status",
    "migrate:reset": "./node_modules/.bin/env-cmd -f .config/.env pnpm --filter ./packages/backend run migrate:reset"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/woodnext/iqbe-dev.git"
  },
  "author": "woodnext",
  "license": "ISC",
  "dependencies": {
    "env-cmd": "^10.1.0",
    "ps-tree": "^1.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.27",
    "backend": "link:packages/backend",
    "iqbe": "link:",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6"
  }
}
